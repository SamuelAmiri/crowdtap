<!DOCTYPE html>
<html>
<head>
  <title></title>
</head>
<body>

  <%= form_tag({:controller=>"restaurants", :action=>"index"}, method: :get) do %>
      <%= text_field_tag :search_term %>
      <%= submit_tag 'Search for a Bar', name: nil %>
  <% end %>

  <% if @place.present? %>
    <%  @place.each do |x| %>
      <%= x.name %> <br/>
      <%= x.vicinity %> <br/>
      <%= x.lat %> <br/>
      <%= x.lng %> <br/>
      <%= x.icon %> <br/>
      <%= x.types %> <br/>
      <%= x.name %> <br/>

      <br>
      <br>
      <br>
    <% end %>
  <% end %>
</body>
</html>